dseg segment
    Switch sword 0
dseg ends

thread main
    SetSensorTouch(IN_1)
    OnFwd(OUT_BC, 100)

CheckSensor:
    ReadSensor(IN_1, Switch) //A variavel switch conter√° 1 ou 0 dependendo do sensor de toque
    brtst EQ, CheckSensor, Switch
    OnRev(OUT_BC,100)
    wait 300
    OnFwd(OUT_B,100)
    wait 300
    OnFwd(OUT_BC,100)
    jmp CheckSensor
endt
